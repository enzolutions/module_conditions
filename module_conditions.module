<?php

/**
 * @file
 * Contains module_conditions.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function module_conditions_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the module_conditions module.
    case 'help.page.module_conditions':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Drupal 8 Plugin condition using Module status') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function module_conditions_theme() {
  $theme = [];

  return $theme;
}
